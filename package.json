{
  "name": "zlib-web-streams",
  "version": "1.0.7",
  "description": "Zlib compression library compiled to WebAssembly",
  "author": "Jean-loup Gailly and Mark Adler (Zlib authors), Gildas Lormeau (zlib-web-streams)",
  "license": "Zlib",
  "main": "src/index.js",
  "type": "module",
  "exports": {
    ".": "./src/index.js",
    "./bundle": "./src/bundle.js",
    "./dist": "./dist/zlib.js",
    "./dist/min": "./dist/zlib.min.js",
    "./wasm": "./src/wasm.js"
  },
  "scripts": {
    "build": "./scripts/build-dist-wasm.sh && vite build",
    "test": "deno run --allow-read --allow-net --allow-run tests/test-runner.js",
    "clean": "rm -rf dist-wasm zlib/*.o zlib/*.a zlib/*.wasm zlib/*.js dist"
  },
  "keywords": [
    "zlib",
    "compression",
    "deflate",
    "deflate64",
    "gzip",
    "inflate",
    "streaming",
    "wasm"
  ],
  "files": [
    "dist/",
    "dist-wasm/",
    "src/"
  ],
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "vite": "^7.0.6"
  }
}
